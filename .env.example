GEMINI_API_KEY=
MAIN_AGENT_PORT=5050
PLAYWRIGHT_AVAILABLE=false

# Database
# For production/managed databases (OVH, AWS RDS, etc.): use sslmode=require
# DATABASE_URL=postgres://USER:PASSWORD@HOST:5432/DATABASE?sslmode=require
# For local development (local PostgreSQL without SSL): use sslmode=disable or omit sslmode
DATABASE_URL=postgres://USER:PASSWORD@localhost:5432/DATABASE

# Authentication
# CRITICAL: Replace this with a secure, randomly generated secret (minimum 32 characters)
# Generate with: openssl rand -base64 32
# Store via environment/secret management - NEVER commit the real secret to git
JWT_SECRET=REPLACE_ME_WITH_SECURE_RANDOM_SECRET_MIN_32_CHARS_a8f9d2e4b1c3
JWT_EXPIRES_IN=7d
NODE_ENV=development
